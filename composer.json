{
    "name": "woohoolabs/worm",
    "description": "Woohoo Labs. Worm ORM",
    "type": "library",
    "keywords": ["Woohoo Labs.", "Worm", "ORM", "Object Relational Mapper", "Data Mapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Máté Kocsis",
            "email": "kocsismate@woohoolabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/woohoolabs/worm/issues",
        "source": "https://github.com/woohoolabs/worm"
    },
    "require": {
        "php": "^7.4.0||^8.0.0",
        "woohoolabs/larva": "^0.7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-phpunit": "^0.12.0",
        "phpstan/phpstan-strict-rules": "^0.12.0",
        "phpunit/phpunit": "^9.5.0",
        "squizlabs/php_codesniffer": "^3.5.0",
        "woohoolabs/coding-standard": "^2.2.0",
        "woohoolabs/releaser": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "WoohooLabs\\Worm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WoohooLabs\\Worm\\Examples\\": "examples/",
            "WoohooLabs\\Worm\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse --level 8 src tests",
        "phpcs": "phpcs"
    },
    "config": {
        "sort-packages": true,
        "platform-check": false
    }
}
